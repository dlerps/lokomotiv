<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard1.3</TargetFrameworks>
    <Authors>Daniel Lerps</Authors>
    <Company />
    <Product />
    <Description>Library to make use of the open APIs of Deutsche Bahn</Description>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/dlerps/lokomotiv/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dlerps/lokomotiv</PackageProjectUrl>
    <RepositoryType>https://github.com/dlerps/lokomotiv</RepositoryType>
    <PackageTags>deutsche bahn,bahn,timetable,library,public transport</PackageTags>
    <Version>1.1.0</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
</Project>